  timing              String
  skill               String
  dfclty              String
  target              String
  rng                 String
  encroach            String
  restrict            String
  premise             String?
  flavor              String?
  effect              String?
  ref_weapon_id       String?
  ref_armor_id        String?
  ref_faq_id       String?
  ref_info_id      String?
  update_supplement              String?
  other_ver_id        String?
  rel_power_id        String?
  rel_weapon_id       String?
  rel_armor_id        String?
  rel_vehicle_id      String?
  rel_connection_id   String?
  rel_general_id      String?
  rel_dlois_id        String?
  type_restrict_order Int
}

